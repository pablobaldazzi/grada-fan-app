# Build variant (rangers, deportes-concepcion). Used by app.config.js for app identity.
APP_VARIANT=rangers

# Backend API base URL
EXPO_PUBLIC_API_BASE_URL=http://localhost:3002

# Club slug override (optional). app.config.js uses APP_VARIANT; this overrides at runtime when set.
EXPO_PUBLIC_CLUB_SLUG=rangers

# Enable demo mode with mock data (set to 'true' for development without backend)
# When true, the app uses mock data and any email/password will work for login
EXPO_PUBLIC_USE_MOCK_DATA=false

# Clerk Authentication
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...
