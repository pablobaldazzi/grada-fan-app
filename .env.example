# Build variant (rangers, deportes-concepcion, palestino). Used by app.config for app identity.
APP_VARIANT=rangers

# Backend API base URL
EXPO_PUBLIC_API_BASE_URL=http://localhost:3002

# Club slug override (optional). Overrides APP_VARIANT-derived slug at runtime when set.
EXPO_PUBLIC_CLUB_SLUG=rangers

# Enable demo mode with mock data (set to 'true' for development without backend)
EXPO_PUBLIC_USE_MOCK_DATA=false

# Clerk Authentication (required for real auth)
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...

# Optional: used by server and build for domain (e.g. Replit). Leave unset for local Expo.
# EXPO_PUBLIC_DOMAIN=
# DATABASE_URL=postgresql://...   # only if using drizzle scripts (e.g. db:push)
